/* swap without holes */

void swap(loc x, loc y)
{ x :-> a ** y :-> b }
{ x :-> b ** y :-> a }
{
  let yy = *y;
  let www = malloc(2);
  let xx = *x;
  *x = yy;
  *y = xx;
  free(www);
}

