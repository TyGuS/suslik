predicate sll(loc x, int len, int lo, int hi) {
|  x == null        => { len == 0  /\  lo == 7  /\  hi == 0 ; emp }
|  not (x == null)  => { len == 1 + len1  /\  0 <= len1  /\  lo == (v <= lo1 ? v : lo1)  /\  hi == (hi1 <= v ? v : hi1) /\ 0 <= v /\ v <= 7;
                      [x, 2] ** x :-> v ** (x + 1) :-> nxt ** sll(nxt, len1, lo1, hi1) }
}