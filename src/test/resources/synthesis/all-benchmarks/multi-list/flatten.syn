void multilist_flatten (loc r) {
  let x = *r;
  if (x == 0) {
    *r = 0;
  } else {
    let h = *x;
    let t = *(x + 1);
    *r = t;
    multilist_flatten(r);
    multilist_flatten116(t, h, x, r);
  }
}

